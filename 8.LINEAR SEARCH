#include<stdio.h>
int main()
{
	int i,n,p,a[20],s,k=0;
	printf("enter the number elements in the list:  ");
	scanf("%d",&n);
	printf("the elements: ");
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	printf("enter the element to search: ");
			scanf("%d",&s);
			for(i=0;i<n;i++)
			{
				if(a[i]==s)
				{
					printf("element %d is found at location %d\n ",s,i+1);
				}
			}
			if(i==0)
			{
				printf("element not found");
			}
			else
			{
				printf("\n%d element is present %d times",s,i);
			}
}
